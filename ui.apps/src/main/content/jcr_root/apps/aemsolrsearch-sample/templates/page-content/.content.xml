        <logo
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemsolrsearch-sample/components/structure/logo"/>
        <topnav
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemsolrsearch-sample/components/structure/topnav"/>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">

        </par>
    </jcr:content>
</jcr:root>
